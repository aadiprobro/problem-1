// Check if the given number is divisible of another number given by user or not

#include <iostream>
using namespace std;

int main()
{
    int a, b;

    cout << "Enter first number : ";
    cin >> a;
    cout << "\nEnter second number : ";
    cin >> b;

    if (a % b == 0)
        cout << "\nThe number is divisible.";
    else
        cout << "\nThe number is not divisible.";

    return 0;
}
